{
	"first_name": {
		"identifier": "first_name",
		"rules": [
			{
				"type": "empty",
				"prompt": "Ingresa tu nombre"
			},
			{
				"type": "maxLength[20]",
				"prompt": "El nombre no puede exceder los 20 caracteres"
			}
		]
	},
	"last_name": {
		"identifier": "last_name",
		"rules": [
			{
				"type": "empty",
				"prompt": "Ingresa tu apellido"
			},
			{
				"type": "maxLength[20]",
				"prompt": "El apellido no puede exceder los 20 caracteres"
			}
		]
	},
	"phone_number": {
		"identifier": "phone_number",
		"rules": [
			{
				"type": "integer",
				"prompt": "El telefono debe contener solo digitos"
			},
			{
				"type": "minLength[6]",
				"prompt": "El telefono debe contener al menos 6 digitos"
			},
			{
				"type": "maxLength[12]",
				"prompt": "El telefono debe contener a lo mas 12 digitos"
			}
		]
	}
}